{
  "include": ["./src/**/*"],
  "compilerOptions": {
    "target": "es2016",
    "module": "es6",
    "declaration": true,
    "outDir": "./dist",
    "sourceMap": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "strictFunctionTypes": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noUncheckedIndexedAccess": true,
  }
}
